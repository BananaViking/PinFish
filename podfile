target 'PinFish' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PinFish
pod 'Alamofire'
pod 'SwiftyJSON'

  target 'PinFishTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PinFishUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
